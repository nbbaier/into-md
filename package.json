{
  "name": "into-md",
  "private": false,
  "bin": {
    "into-md": "dist/index.mjs"
  },
  "type": "module",
  "module": "src/index.ts",
  "scripts": {
    "dev": "alchemy dev",
    "deploy": "alchemy deploy",
    "destroy": "alchemy destroy",
    "start": "bun run src/index.ts",
    "build": "tsdown",
    "build:watch": "tsdown --watch",
    "test": "bun test",
    "check": "ultracite check",
    "fix": "ultracite fix",
    "typecheck": "tsgo --noEmit",
    "release:minor": "npm version minor -m 'v%s' && bun run build",
    "release:major": "npm version major -m 'v%s' && bun run build",
    "release:patch": "npm version patch -m 'v%s' && bun run build"
  },
  "dependencies": {
    "@mozilla/readability": "^0.6.0",
    "@types/turndown": "^5.0.6",
    "alchemy": "^0.84.0",
    "cheerio": "^1.2.0",
    "commander": "^14.0.3",
    "hono": "^4.11.9",
    "jsdom": "^28.0.0",
    "playwright": "^1.58.2",
    "turndown": "^7.2.2"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.15",
    "@types/bun": "^1.3.9",
    "@types/node": "^25.2.3",
    "@typescript/native-preview": "^7.0.0-dev.20260212.1",
    "oxlint": "^1.47.0",
    "tsdown": "^0.20.3",
    "ultracite": "7.1.5"
  },
  "peerDependencies": {
    "typescript": "^5.9.3"
  },
  "version": "0.4.0"
}
